{
  "editor.rulers": [
    78,
    80
  ],
  "files.exclude": {
    "**/*.lo": true,
    "**/*.o": true,
    "**/*.gcno": true
  },
  "[c]": {
    "editor.tabSize": 8,
    "editor.insertSpaces": false
  },
  "[cpp]": {
    "editor.tabSize": 8,
    "editor.insertSpaces": false
  },
  "[javascript]": {
    "editor.tabSize": 2,
    "editor.insertSpaces": true
  },
  "[xml]": {
    "editor.tabSize": 2,
    "editor.insertSpaces": true
  },
  "[css]": {
    "editor.tabSize": 2,
    "editor.insertSpaces": true
  },
  "files.associations": {
    "unistd.h": "c",
    "errno.h": "c",
    "fcntl.h": "c",
    "metrics.h": "c",
    "painter.h": "c"
  }
}